{
  "name": "express-prisma-starter",
  "version": "1.0.0",
  "description": "Express JS with Prisma starter",
  "main": "dist/index.js",
  "author": "IyanSR",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "nodemon",
    "lint": "eslint --ignore-path .eslintignore \"src/**/*.ts\" --fix",
    "lint:check": "eslint --ignore-path .eslintignore \"src/**/*.ts\"",
    "prepare": "husky install",
    "build": "NODE_ENV=production && esbuild index.ts --platform=node --bundle --format=esm --splitting --minify --outdir=dist",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "morgan": "^1.10.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "esbuild": "^0.17.18",
    "esbuild-node-tsc": "^2.0.5",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prettier-eslint": "^15.0.1",
    "prisma": "^4.13.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier-eslint --write"
    ]
  }
}